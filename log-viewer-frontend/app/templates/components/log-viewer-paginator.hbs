<ul class="pagination">
  <li class="log-viewer-table__paginator-prev-button {{if disablePrevButton 'disabled' 'waves-effect'}}"><a {{action 'goToPrevPage'}}><i class="material-icons">chevron_left</i></a></li>
  {{#if (gt totalPages 0)}}
    {{#each (range 1 totalPages true) as |i|}}
      <li class="log-viewer-table__paginator-page-button {{if (eq i currPage) 'active' 'waves-effect'}}"><a {{action 'goToPage' i}}>{{i}}</a></li>
    {{/each}}
  {{else}}
    <li class="active"><a>1</a></li>
  {{/if}}
  <li class="log-viewer-table__paginator-next-button {{if disableNextButton 'disabled' 'waves-effect'}}"><a {{action 'goToNextPage'}}><i class="material-icons">chevron_right</i></a></li>
</ul>